<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com https://www.woltlab.com/XSD/2019/option.xsd">
	<import>
		<categories>
			<category name="syn_discord_guild_info_box.general">
				<parent>discord</parent>
				<showorder>5</showorder>
			</category>
		</categories>
		<options>
			<option name="syn_discord_guild_info_box_api_id">
				<categoryname>syn_discord_guild_info_box.general</categoryname>
				<optiontype>synDiscordGuildInfoApiSelect</optiontype>
				<validationpattern>\d+</validationpattern>
				<showorder>1</showorder>
			</option>
			<option name="syn_discord_guild_info_box_server_id">
				<categoryname>syn_discord_guild_info_box.general</categoryname>
				<optiontype>synDiscordGuildInfoBoxSelect</optiontype>
				<validationpattern>\d+</validationpattern>
				<showorder>2</showorder>
			</option>
			<option name="syn_discord_guild_info_box_invite_url">
				<categoryname>syn_discord_guild_info_box.general</categoryname>
				<optiontype>URL</optiontype>
				<validationpattern>^$|^https?://</validationpattern>
				<showorder>3</showorder>
				<disableAutocomplete>1</disableAutocomplete>
			</option>
			<option name="syn_discord_guild_info_box_invite_new_window">
				<categoryname>syn_discord_guild_info_box.general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<showorder>4</showorder>
			</option>
		</options>
	</import>
</data>
